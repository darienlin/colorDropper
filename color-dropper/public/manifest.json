{
    "manifest_version": 3,
    "name": "Color Dropper",
    "description": "color dropper extension",
    "version": "1.0",
    "icons": {
      "32": "dropper32.png"
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "storage",
      "scripting",
      "tabs",
      "activeTab"
    ],
    "web_accessible_resources": [
    {
      "resources": [ "dropper16.png" ],
      "matches": [ "https://*/*", "http://*/*"]
    }
  ]
  }